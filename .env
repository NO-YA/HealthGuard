cat > .env << 'EOF'
# Flask
FLASK_ENV=development
FLASK_DEBUG=True
PORT=5000

# MongoDB
MONGO_URI=mongodb://admin:password@mongodb:27017/healthguard?authSource=admin
MONGO_DB_NAME=healthguard

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Redis
REDIS_URL=redis://redis:6379/0
REDIS_ENABLED=False
EOF